<Window x:Class="prac13.PassWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prac13"
        mc:Ignorable="d"
       Title="Введите пароль" Height="113" Width="221" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Activated="Window_Activated" Closing="Закрытие">
    <StackPanel>
        <PasswordBox x:Name="Pass" HorizontalAlignment="Center" VerticalAlignment="Top" Width="210" Height="26"/>
        <Button Content="Войти" Width="210" Height="25" Click="Vvod" IsDefault="True"/>
        <Button Content="Отмена" HorizontalAlignment="Center" Margin="0,0,0,0" Width="210" Height="35" Click="Cancel"/>
    </StackPanel>
</Window>
